    print(\"{0}.{1} -- {2} -- {3}\".format(no, MUSIC_BOX[n].music_info[0], MUSIC_BOX[n].music_info[2],\r\n                                                 MUSIC_BOX[n].music_info[3]))\r\n            no += 1\r\n        print(\"------------------------------------------------\")\r\n        _ch_num = (input(\"请输入序号（输入0返回主菜单）：\"))\r\n        if _ch_num == \"0\":\r\n            display_total_info()\r\n        else:\r\n            MUSIC_BOX[int(_ch_num) - 1].function()\r\n    else:\r\n        os.system(\"cls\")\r\n        input(\"★★提示：播放列表为空！\")\r\n        display_total_info()\r\n\r\n\r\ndef judge_music_mode():\r\n    pass\r\n\r\nif __name__ == \"__main__\":\r\n    display_total_info()\r\n
